        <script><![CDATA[var NeedItUtils = Class.create();
NeedItUtils.prototype = {
    initialize: function() {
    },

	isDatePast: function(strDate){
	// Create GlideDateTime objects for the current date and the passed in date
	var nextpossibledate = new GlideDateTime();
	
	gs.addInfoMessage(`Current Date and Time is: ${nextpossibledate}`);

	nextpossibledate.addDaysLocalTime(+1);
	gs.addInfoMessage(`Next Possible date is: ${nextpossibledate}`);
	var testdate = new GlideDateTime(strDate);
		// If the testdate is before nextpossibledate, return true, else return false
		if (testdate.before(nextpossibledate)) {
			return true;
		}
		else {
			return false;
		}
	},

    type: 'NeedItUtils'
};]]></script>
